generator client {
   provider = "prisma-client-js"  
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}
model Apartment {
  id               Int      @id @default(autoincrement())
  unit_name        String
  unit_number      String
  unit_description String
  project          String
  price            Int
  city             String
  createdAt        DateTime @default(now())
}